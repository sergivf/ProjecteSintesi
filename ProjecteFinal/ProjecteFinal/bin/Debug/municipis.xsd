<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" attributeFormDefault="unqualified" elementFormDefault="qualified">
	<xs:element name="municipis" type="municipisType"/>
	<xs:complexType name="municipisType">
	  <xs:sequence>
		  <xs:element name="municipi" type="municipiType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="municipiType">
		<xs:sequence>
			<xs:element name="codimunicipi" type="xs:integer"/>
			<xs:element name="nommunicipi" type="xs:string"/>
			<xs:element name="abrev" type="xs:string" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>